    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "soft-delete-plugin-mongoose": "^1.0.15",
    "swagger-ui-express": "^4.5.0",
    "twilio": "^3.80.0",
